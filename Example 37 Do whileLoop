import java.util.*;
public class PrintEvenNumbers {
    public static void main(String[] args) {
Scanner sc= new Scanner(System.in);
        int i = 1;
        System.out.println("Even numbers from 1 to 20:");
        do {
            if (i % 2 == 0) {
                System.out.print(i + " ");
            }
            i++;
        } while (i <= 20);
    }
}
